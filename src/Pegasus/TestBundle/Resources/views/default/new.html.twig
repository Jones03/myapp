{% extends 'PegasusTestBundle::layout.html.twig' %}
{% block title %}Form 1{% endblock %}
{% block content %}
<form action="{{ path('_form1') }}" method="POST" {{ form_enctype(form) }}>
{{ form_errors(form) }}

<div>
    {{ form_label(form.task) }}
    {{ form_errors(form.task) }}
    {{ form_widget(form.task) }}
</div>

<div>
    {{ form_label(form.dueDate) }}
    {{ form_errors(form.dueDate) }}
    {{ form_widget(form.dueDate) }}
</div>

{{ form_rest(form) }}
        <input type="submit" />
</form>
{% endblock %}